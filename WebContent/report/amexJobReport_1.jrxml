	<parameter name="cycleDate" class="java.lang.String">
		<defaultValueExpression><![CDATA["2011-11-17"]]></defaultValueExpression>
	</parameter>
	<parameter name="logisticName" class="java.lang.String">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[select j.JOB_BAG_NO, SUBSTRING(j.AFP_NAME,11,14) JOB_NAME, j.idf_JOB_CODE_NO,  SUBSTRING(j.AFP_NAME,26,8) + SUBSTRING(j.AFP_NAME,36,3) shortAfpName ,  j.AFP_NAME, j.ACCOUNTS, j.PAGES, j.SHEETS, j.MRDF
from job_bag j
where (j.idf_CUST_NO='AE' and j.AFP_NAME <>''  and  j.LOG_FILENAME='$P!{CSVName}' and convert(varchar(10),CYCLE_DATE,120)='$P!{cycleDate}')
union
select j.JOB_BAG_NO, j.PROG_NM JOB_NAME, j.idf_JOB_CODE_NO,  j.afp_name shortAfpName ,  j.AFP_NAME, j.ACCOUNTS, j.PAGES, j.SHEETS, j.MRDF
from job_bag j
where (j.idf_CUST_NO='AE' and j.AFP_NAME <>''  and  j.LOG_FILENAME like '$P!{logisticName}' and convert(varchar(10),CYCLE_DATE,120)='$P!{cycleDate}')
and (j.IS_DELETED = 0 or j.IS_DELETED is null)
order by  JOB_NAME asc, shortAfpName, AFP_NAME]]>
	</queryString>

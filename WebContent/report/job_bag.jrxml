from job_bag j
join job_bag_splite js on js.IDF_JOB_BAG_NO = j.JOB_BAG_NO
join customer c on j.idf_CUST_NO = c.CUST_NO
left outer join code c_PRINT_TYPE on c_PRINT_TYPE.id = lp_CODE_PRINT_TYPE
left outer join code c_CODE_PRINTER on c_CODE_PRINTER.id = lp_CODE_PRINTER
left outer join code c_CODE_PS_TYPE on c_CODE_PS_TYPE.id = ps_CODE_PS_TYPE
left outer join code c_CODE_LG_TYPE on c_CODE_LG_TYPE.id = j.CODE_LG_TYPE
left outer join code c_CODE_MAIL_TO_POSTOFFICE on c_CODE_MAIL_TO_POSTOFFICE.id = j.CODE_MAIL_TO_POSTOFFICE
left outer join code c_CODE_MAIL_CATEGORY on c_CODE_MAIL_CATEGORY.id = j.CODE_MAIL_CATEGORY
left outer join code c_CODE_DISPATCH_TYPE on c_CODE_DISPATCH_TYPE.CODE_KEY = j.DISPATCH_TYPE
